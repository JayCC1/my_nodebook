{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"], // 指定要包含在编译中的库文件
    "declaration": true, // 生成相应的 '.d.ts' 文件
    "emitDeclarationOnly": true,
    "outDir": "es",
    "types": ["node", "@vue/runtime-core"] // 需要包含的类型声明文件名列表
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}
