  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@jaycce/gulp-utils": "workspace:^",
    "element-plus": ">=2.3.14",
    "vue": ">=3.2.0"
  },
